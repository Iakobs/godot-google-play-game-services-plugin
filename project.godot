; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "AndroidPluginClient",
"language": "GDScript",
"path": "res://addons/google_play_game_services_manager/AndroidPluginClient.gd"
} ]
_global_script_class_icons={
"AndroidPluginClient": ""
}

[application]

config/name="Google Play Game Services Demo"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

GamesSignInClient="*res://addons/google_play_game_services_manager/GamesSignInClient.gd"

[display]

window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/google_play_game_services_manager/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
